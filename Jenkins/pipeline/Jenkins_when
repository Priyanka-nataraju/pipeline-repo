pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
                git branch: 'main', 
                credentialsId: 'github', 
                url: 'https://github.com/Priyanka-nataraju/pipeline-repo.git'
            }
        }

        stage('BUILD'){
                when {
                    branch 'main'
                }
                steps{
                    echo " Building"
                }
        }
    }
}