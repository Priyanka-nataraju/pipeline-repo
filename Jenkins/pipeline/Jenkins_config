pipeline{
    agent any
    stages{
        stage('CONFIG'){
            steps{
                git branch: 'main', credentialsId: 'github', url: 'https://github.com/Priyanka-nataraju/pipeline-repo.git'
            }
        }

        stage("CHECK CONFIG")
        steps{
            checkout scmGit(branches: [[name: '*/main']], extensions: [], 
            userRemoteConfigs: [[credentialsId: 'github', 
            url: 'https://github.com/Priyanka-nataraju/pipeline-repo.git']])
        }
    }
}